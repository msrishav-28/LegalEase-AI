# Database Configuration (Railway will provide these)
DATABASE_URL=postgresql://username:password@host:port/database
POSTGRES_DB=legalease_ai
POSTGRES_USER=legalease_user
POSTGRES_PASSWORD=your_secure_password

# Redis Configuration (Railway will provide)
REDIS_URL=redis://host:port

# RabbitMQ Configuration (Railway will provide)
RABBITMQ_URL=amqp://user:password@host:port/

# Application Configuration
SECRET_KEY=your-super-secret-key-change-this-in-production
JWT_SECRET_KEY=your-jwt-secret-key-change-this
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Environment
ENVIRONMENT=production
DEBUG=false

# CORS Configuration
ALLOWED_ORIGINS=https://your-frontend-domain.railway.app

# File Upload Configuration
MAX_FILE_SIZE=10485760
UPLOAD_DIR=/app/uploads

# AI/ML Configuration
OPENAI_API_KEY=your-openai-api-key
HUGGINGFACE_API_KEY=your-huggingface-api-key

# Celery Configuration
CELERY_BROKER_URL=amqp://user:password@host:port/
CELERY_RESULT_BACKEND=redis://host:port

# Logging
LOG_LEVEL=INFO